                         Month == "februari" ~2,
                         Month == "mars" ~ 3,
                         Month == "april" ~ 4,
                         Month == "maj" ~ 5,
                         Month == "juni" ~ 6,
                         Month == "juli" ~ 7,
                         Month == "augusti" ~ 8,
                         Month == "september" ~ 9,
                         Month == "oktober" ~ 10,
