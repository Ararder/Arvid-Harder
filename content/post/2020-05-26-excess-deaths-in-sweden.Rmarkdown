  select(-...24, -...25 , -...26, -...27)

#removing 2 extraneous rows
deaths_by_community <- deaths_by_community[-1,]
deaths_by_community <- deaths_by_community[-1,]
#aggregated info, removing it from clean dataset. saving it as misc
